{
  "devDependencies": {
    "@types/node": "^20.14.11",
    "esbuild": "^0.23.0",
    "tsx": "^4.16.2"
  },
  "name": "tiny-stable-stringify",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "test": "tsx test/test.ts",
    "build:js": "esbuild src/stable.ts --format=esm --outfile=dist/index.js --sourcemap --target=node18",
    "build:cjs": "esbuild src/commonjs.js --bundle --format=cjs --outfile=dist/index.cjs --sourcemap --target=node12",
    "prepublishOnly": "node --run build:cjs && node --run build:js"
  },
  "author": "Sam Thorogood <sam.thorogood@gmail.com>",
  "license": "Apache-2.0",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./types.d.ts"
  },
  "types": "./types.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs"
}
